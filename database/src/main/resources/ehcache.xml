<?xml version="1.0" encoding="UTF-8"?>
<ehcache>

	
	<diskStore path="d:/ehcache/" />
	
	<defaultCache maxElementsInMemory="10" overflowToDisk="true" eternal="false"
		memoryStoreEvictionPolicy="LRU" maxElementsOnDisk="10000000" diskExpiryThreadIntervalSeconds="600"
		timeToIdleSeconds="60" timeToLiveSeconds="120" diskPersistent="true" />

</ehcache>